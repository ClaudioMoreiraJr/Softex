var varBoolean;
var varBigint;
var varString;
var varSymbol;

console.log("Valores iniciais:");
console.log("varBoolean:", varBoolean);
console.log("varBigint:", varBigint);
console.log("varString:", varString);
console.log("varSymbol:", varSymbol);

varBoolean = null;
varBigint = null;
varString = null;
varSymbol = null;

console.log("\nValores após definir como null:");
console.log("varBoolean:", varBoolean);
console.log("varBigint:", varBigint);
console.log("varString:", varString);
console.log("varSymbol:", varSymbol);

varBoolean = true;
varBigint = 1234567890123456789012345678901234567890n;
varString = "Isso é uma string";
varSymbol = Symbol("Isso é um símbolo");

console.log("\nValores após atribuir valores:");
console.log("varBoolean:", varBoolean);
console.log("varBigint:", varBigint);
console.log("varString:", varString);
console.log("varSymbol:", varSymbol);
